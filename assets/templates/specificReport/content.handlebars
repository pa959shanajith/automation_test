<body>
   <span class='hide remarksLength'>{{remarksLength}}</span>
   <span class='hide commentsLength'>{{commentsLength}}</span>
   <span class='hide errormessage'>{{error.emsg}}</span>
   <div class="mainContainer">

      {{#each overallstatus}}
      <div class="hearderCon">
         <div class="headerRepDetails">
            <label>Date Generated: </label>
            <span>{{date}} {{time}}</span>
         </div>
         <div class="headerRepDetails">
            <label>Module Name: </label>
            <span class='moduleEllipsis' title="{{moduleName}}">{{moduleName}}</span>
         </div>
         <div class="h2Header">
            <h4>Avo Assure Execution Report | Scenario</h4>
         </div>
         <div class="h2Header" style="left:78%;user-select:none">
            <label for="scrShotChk" title="Include Screenshots in PDF" style="font-size:13px">With Screenshots</label>
            <input id="scrShotChk" type="checkbox" title="Include Screenshots in PDF" style="height:12px;margin:0" checked="true">
            <img alt="Save PDF" class="export-icons" data-rep="pdf" src="../static/imgs/ic-pdf.png" title="Save PDF Report">
            <img alt="Export JSON" class="export-icons" data-rep="json" style="" src="../static/imgs/ic-export-to-json.png" title="Export to JSON">
            {{#ifnotEquals video '-'}}
               <img alt="Download video" class="export-icons" data-rep="mp4" data-path="{{video}}" style="" src="../static/imgs/screenrecord.png" title="Download Execution Video">
            {{/ifnotEquals}}
         </div>
      </div>
      <button class="collapsible">
         <span style="left: 40px;position: relative">
            <span class="arrow-down"></span><span class='resultSummary'>Result Summary</span></span>
      </button>
      <div class='resultSummaryBox' style="width: 100%;">
         <div class="leftSummary">
            <label>Scenario</label>
            <span class="scenarioName">: {{scenarioName}}</span>
            <label>Cycle</label>
            <span>: {{cycleName}}</span>
            <label>Release</label>
            <span>: {{releaseName}}</span>
            <label>Project</label>
            <span>: {{projectName}}</span>
            <label>Domain</label>
            <span>: {{domainName}}</span>
            <label>Browser Type</label>
            <span class="capitalize">: {{browserType}}</span>
            <label>Browser Version</label>
            <span>: {{browserVersion}}</span>
            {{#ifnotEquals version undefined}}
               {{#ifnotEquals version '-'}}
                  <label>Version</label><span>: {{version}}</span>
               {{/ifnotEquals}}
            {{/ifnotEquals}}
            <span style='visibility:hidden' class="reportId">{{reportId}}</span>
            <span style='visibility:hidden' class="executionId">{{executionId}}</span>
         </div>
         <div class="leftSummary rightBox">
            <label>Overall Status</label>
            <span class='overallStatusVal' style="color:{{getColor overallstatus}}"><text style='color:#000; !important'>:</text> {{overallstatus}}</span>
            <label class='toggleIncompleteStatus'>Testcase Status: </label>
            <span class="progress customprogress toggleIncompleteStatus">
               <span class="progress-bar" role="progressbar" style="background-color:#28a745;width:{{pass}}%">
               </span>
               <span class="progress-bar" role="progressbar" style="background-color:#dc3545;width: {{fail}}%">
               </span>
               <span class="progress-bar" role="progressbar" style="background-color:#ffc107;width: {{terminate}}%">
               </span>
            </span>
            <label></label>
            <span class="progressPercentBox toggleIncompleteStatus">
               <text style="color:#28a745;" class='progVal'>Pass: {{pass}}%</text>
               <text style="color:#dc3545;" class='progVal'>Fail: {{fail}}%</text>
               <text style="color:#ffc107;" class='progVal'>Terminated: {{terminate}}%</text>
            </span>
            <label class='toggleIncompleteStatus'>Start Date and Time </label>
            <span class='toggleIncompleteStatus'>: {{StartTime}}</span>
            <label class='toggleIncompleteStatus'>End Date and Time </label>
            <span class='toggleIncompleteStatus'>: {{EndTime}}</span>
            <label class='toggleIncompleteStatus'>Time Elapsed </label>
            <span class='toggleIncompleteStatus'>: {{EllapsedTime}}</span>
         </div>
      </div>
      {{/each}}
      <div class='buttonDivBox'>
         <button class="logintojira" title="Login to Jira">Jira Login</button>
         <button class="disconnectJira" title="Disconnect from Jira">Disconnect</button>
         <label class="Llogdefect">Log Defect</label>
         <input type="text" id="inputSlno" placeholder="Enter SL No." maxlength="5" />
         <button class="go_defectlog opendescpopup">Go</button>
      </div>
      <!-- Reports Table -->
      <div class="maintabCont_collapse">
         <table id="specificReportDataTable" class="display responsive wrap" style="width:100%;table-layout:fixed;">
            <thead>
               <tr>
                  <th>Sl No.</th>
                  <th>Test Step No.</th>
                  <th style="display:none">Test Step Description</th>
                  <th>Expected Result</th>
                  <th>Actual Result</th>
                  <th>Status</th>
                  <th>Elapsed Time</th>
                  <th class='toggleCommentsCol'>Comments</th>
                  <th>JIRA Defect ID</th>
               </tr>
            </thead>
            <tbody>
               {{#each rows}}
               <tr style="width: 100%;" class="reportdetailsrow" data-id="{{id}}" data-parentid="{{parentId}}">
                  <td class="rDid tabCont">{{slno}}</td>
                  {{#ifEquals status "Skipped"}}
                     <td class="rDstep tabCont iteration"></td>
                  {{/ifEquals}}
                  {{#ifnotEquals status "Skipped"}}
                  <td class="rDstep tabCont iteration">{{Step}}</td>
                  {{/ifnotEquals}}
                  <td class="{{getClass StepDescription}}"
                     style="display:none;width:0px; padding-left: 10px; font-weight:{{getStyle StepDescription}}">
                     {{StepDescription}}
                  </td>
                  {{#ifEquals testcase_details.testcaseDetails undefined}}
                  <td class="rDExpectedRes tabCont {{getClass StepDescription}}">{{StepDescription}}</td>
                  {{/ifEquals}} {{#ifEquals testcase_details.testcaseDetails ""}}
                  <td class="rDExpectedRes tabCont">{{StepDescription}}</td>
                  {{/ifEquals}} {{#ifnotEquals testcase_details.testcaseDetails ""}}
                  {{#ifnotEquals testcase_details undefined}}
                  <td class="rDExpectedRes tabCont">{{testcase_details.testcaseDetails}}</td>
                  {{/ifnotEquals}} {{/ifnotEquals}} {{#ifEquals status undefined}}
                  <td class="rDActualRes tabCont"></td>
                  {{/ifEquals}} {{#ifEquals status ""}}
                  <td class="rDActualRes tabCont"></td>
                  {{/ifEquals}} {{#ifEquals status "Pass"}} {{#ifEquals testcase_details.actualResult_pass ""}}
                  <td class="rDActualRes tabCont">
                     {{#ifEquals screenshot_path undefined}}
                     <text class="rDstatus rDstatusLnk" style='text-decoration:none !important;cursor:default;'>As Expected</text>
                     {{/ifEquals}} {{#ifnotEquals screenshot_path undefined}}
                     <text class="rDstatus openscreenshot"  style='text-decoration:none !important;cursor:default;'>As Expected</text>
                     {{/ifnotEquals}}
                  </td>
                  {{/ifEquals}} {{#ifnotEquals testcase_details.actualResult_pass ""}}
                  <td class="rDActualRes tabCont">
                     {{testcase_details.actualResult_pass}}
                  </td>
                  {{/ifnotEquals}} {{/ifEquals}} {{#ifEquals status "Fail"}}
                  {{#ifEquals testcase_details.actualResult_fail ""}}
                  <td class="rDActualRes tabCont">
                     {{#ifEquals screenshot_path undefined}}
                     <text class="rDstatus rDstatusLnk" style="color:{{getColor status}};">{{status}}</text>
                     {{/ifEquals}} {{#ifnotEquals screenshot_path undefined}}
                     <text class="rDstatus openscreenshot" style="color:{{getColor status}};"
                        data-screenshot="{{screenshot_path}}">{{status}}</text>
                     {{/ifnotEquals}}
                  </td>
                  {{/ifEquals}} {{#ifnotEquals testcase_details.actualResult_fail ""}}
                  <td class="rDActualRes tabCont">
                     {{testcase_details.actualResult_fail}}
                  </td>
                  {{/ifnotEquals}} {{/ifEquals}}
                  {{#ifEquals status "Terminate"}}
                     <td class="rDActualRes tabCont">{{StepDescription}}</td>
                   {{/ifEquals}}
                   {{#ifEquals status "Skipped"}}
                     <td class="rDActualRes tabCont">Skipped</td>
                   {{/ifEquals}}
                  <td class="rDstatus tabCont openscreenshot" style="color:{{getColor status}};"
                     data-screenshot="{{screenshot_path}}">{{status}}</td>
                  <td class="rDellaps tabCont">{{EllapsedTime}}</td>
                  <td style='display:none !important' class="rDcomments tabCont">{{Comments}}</td>
                  <td class="rjira tabCont">
                     <span class="opendescpopup" title="Create Issue">{{jira_defect_id}}</span>
                  </td>
               </tr>
               {{/each}}
            </tbody>
         </table>
      </div>
      <!-- Reports Table -->
   </div>
   <div id="overlay"></div>
   <!-- Jira login window popup-->
   <div class="jiraWindow" style="display: none;">
      <div class="popupheader">
         <label>Login to Jira</label>
         <span class="closeWindow">X</span>
      </div>
      <input type="text" class="jirapopupinput" placeholder="Enter Url" id="jiraURL" />
      <input type="text" class="jirapopupinput" placeholder="Enter User Name" id="jiraUserName" />
      <input type="password" class="jirapopupinput" placeholder="Enter Password" id="jiraPassword" />
      <button type="button" class="resetJiraCredentials">Reset</button>
      <button type="button" class="loginJiraCredentials">Connect</button>
      <span class="error-msg"></span>
   </div>
   <!-- Jira description window popup-->
   <div class="createIssueWindow" style="display: none;">
      <div class="popupheader">
         <label>Create Issue</label>
         <span class="closeWindow">X</span>
      </div>
      <p>
         <label>Projects
            <span class="mandatory">*</span>
         </label>
         <select class="jirapopupinput" id="jProjects">
            <option value="" selected disabled>Select Projects</option>
         </select>
      </p>
      <p>
         <label>Issue Type
            <span class="mandatory">*</span>
         </label>
         <select class="jirapopupinput" id="jIssuetype">
            <option value="" selected disabled>Select Issue</option>
         </select>
      </p>
      <p id='parentIssue'>
         <label>Parent Issue Id
            <span class="mandatory">*</span>
         </label>
         <input type="text" class="jirapopupinput" id="jpIssueid" />
      </p>
      <p>
         <label>Summary
            <span class="mandatory">*</span>
         </label>
         <textarea type="text" class="jirapopupinput" id="jSummary" maxlength="255"
            title="Maximum 255 characters accepted."></textarea>
      </p>
      <p>
         <label>Description</label>
         <textarea class="jirapopupinput" id="jDescription"></textarea>
      </p>
      <p>
         <label>Priority
		     <span class="mandatory">*</span>
		 </label>
         <select class="jirapopupinput" id="jPriority">
            <option value="" selected disabled>Select Priority</option>
         </select>
      </p>
      <p>
         <label>Label</label>
         <input class="jirapopupinput" id="jLabel"/>
      </p>
      <p>
         <label>Attachment Path</label>
         <input type="text" id="scrnShotPath" class="jirapopupinput" />
      </p>
      <p class='btn-footer'>
         <span class="error-msg"></span>
         <button type="button" class="cancelIssues">Cancel</button>
         <button type="button" class="createIssues">Create</button>
      </p>
   </div>
   <!-- Ststus popup window -->
   <div class="statusWindow" style="display: none;">
      <div class="popupheader">
         <label>Create Issue</label>
         <span class="closeWindow">X</span>
      </div>
      <p>
         <label id="showStatus"></label>
      </p>
      <p>
         <button type="button" class="closeWindow">Ok</button>
      </p>
   </div>
   <script type="text/javascript" src="../static/reports/specificReport.js"></script>
</body>

</html>