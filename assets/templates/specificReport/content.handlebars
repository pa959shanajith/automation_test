   <style>
      .no-disp {
         display: none !important;
      }

      .unexpand,
      .expand {
         cursor: pointer;
      }

      body {
         font-family: LatoWeb;
         height: 98%;
         overflow: hidden;
      }

      .mainContainer {
         border: 1px solid #B5AAAA;
         margin: 4px;
         float: left;
         padding: 14px;
         width: 97%;
         height: calc(100% - 30px);
      }

      .hearderCon {
         position: relative;
         width: 100%;
         float: left;
         background: #F3F3F3;
         height: 50px;
         margin-bottom: 10px;
         border-radius: 5px;
         font-size: 16px;
         padding-top: 7px;
         color: #5A5A5A;
         ;
      }

      .headerRepDetails {
         width: 50%;
         padding: 3px 40px;
         font-family: Latowebmedium;
      }

      .headerRepDetails label {
         text-align: right;
         width: 29%;
         font-size: 13px;
      }

      .headerRepDetails span {
         width: 64%;
         padding-bottom: 5px;
         font-size: 13px;
         font-family: Latowebmedium;
      }

      .h2Header {
         position: absolute;
