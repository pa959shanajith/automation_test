      }

      .summaryCon span {
         position: absolute;
         top: 10px;
         left: 40px;
      }

      .midSection {
         float: left;
         width: 100%;
      }

      .midContainer {
         border-collapse: collapse;
         border: 1px solid #cfcfcf;
         width: 100%;
         font: 14px cabin_Regular;
         float: left;
         margin: 0 0 1% 0;
         font-family: LatoWeb;
         font-size: 14px;
      }

      .midContainer td {
