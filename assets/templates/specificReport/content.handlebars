      }

      .openscreenshot:hover {
         text-decoration: underline;
      }

      .progress {
         overflow: hidden;
      }

      .progress-bar {
         float: left;
         height: 100%;
         font-size: 12px;
         line-height: 20px;
         color: #fff;
         text-align: center;
         box-shadow: inset 0 -1px 0 rgba(0, 0, 0, .15);
