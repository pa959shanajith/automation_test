<div class="second" >
  <canvas id="{{pieChartID}}" width="100%" height="100%;"></canvas>
</div>
<script>

var ctx = document.getElementById("{{pieChartID}}").getContext('2d');
var config{{pieChartID}} = {
      type: 'pie',
    data: {
    datasets: [{
        data: [{{values}}],
        backgroundColor: [
         "#2ecc71",
         "#3498db",
         "#95a5a6",
         "#9b59b6",
         "#f1c40f",
         "#e74c3c",
         "#34495e"
       ]
    }],
    // These labels appear in the legend and in the tooltips when hovering different arcs
    labels: [{{#each labels}} '{{this}}' ,{{/each}}]
},
    options: {
      maintainAspectRatio: true,
        legend:{
          display: false
        },
        responsive: true
    }
}
var {{pieChartID}} = new Chart(ctx, config{{pieChartID}});
</script>
